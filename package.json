{
  "dependencies": {
    "ace-css": "1.1",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "elm-webpack-loader": "^4.3.1",
    "file-loader": "^0.11.2",
    "foreman": "^2.0.0",
    "json-server": "0.9.5",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.3",
    "webpack-dev-server": "^2.7.1"
  },
  "scripts": {
    "api": "node ./db/api.js",
    "build:staging": "webpack --env=staging --progress --profile --colors",
    "build:prod": "webpack --env=prod --progress --profile --colors",
    "client": "webpack-dev-server --env=dev --port 4200",
    "deploy:staging": "yarn build:staging; surge ./dist -d your-domain.surge.sh",
    "deploy:prod": "yarn build:prod; aws s3 sync --acl public-read --sse --delete ./dist s3://your-domain",
    "start": "nf start"
  }
}
