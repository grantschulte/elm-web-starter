{
  "name": "elm-webpack-starter",
  "version": "0.0.1",
  "description": "SaPP is an Elm web app starter for quick prototyping. This starter uses Webpack for compiling code, Basscss and Google Material Icons for UI scaffolding, Github Pages and Surge.sh for easy deployment to testing environments, and json server for faking a RESTful API implementation.",
  "main": "./src/index.js",
  "author": "Grant J. Schulte <schulte.grant@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/grantschulte/elm-webpack-starter.git"
  },
  "dependencies": {
    "ace-css": "1.1",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "elm-webpack-loader": "^4.3.1",
    "file-loader": "^0.11.2",
    "foreman": "^2.0.0",
    "gh-pages": "1.0.0",
    "json-server": "0.9.5",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.3",
    "webpack-dev-server": "^2.7.1"
  },
  "scripts": {
    "api": "node ./db/api.js",
    "build:staging": "webpack --env=staging --progress --profile --colors",
    "build:prod": "webpack --env=production --progress --profile --colors",
    "client": "webpack-dev-server --env=development --port 4200",
    "deploy:gh-pages": "./deploy-gh-pages.sh",
    "deploy:staging": "yarn build:staging; surge ./dist -d elm-web-starter.surge.sh",
    "start": "nf start"
  },
  "engines": {
    "node": "8.4.0"
  }
}
